{
    "name": "Codeforces Scoreboard",
    "description": "Decorating Codeforces's Scoreboard",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://codeforces.com/contest/*/standings*"
            ],
            "run_at": "document_idle",
            "js": [
                "Script/util.js",
                "Script/problem.js", "Script/user.js", "Script/hack.js", "Script/submission.js",
                "Script/row.js", "Script/scoreboard.js",
                "Script/main.js"
            ]
        }
    ]
}